#!/usr/bin/env coffee
# vim:ft=coffee
# -*- mode:coffee -*-

pushFullWidthSpace = (str) ->
    ((str.split "\n").map (line) ->
        if ((line.charAt 0) == "「") or ((line.charAt 0) == "『")
            line
        else
            "　" + line
    ).join "\n"

main = () ->
    fs = require "fs"
    input = fs.readFileSync "/dev/stdin", "utf-8"
    process.stdout.write pushFullWidthSpace input

main()
